{
  "name": "mpd.fm-frontend",
  "version": "0.2.0",
  "description": "A MPD web server and client to listen to your favorite online radio stations",
  "type": "module",
  "scripts": {
    "build": "webpack --config webpack.dev.cjs",
    "start": "concurrently \"webpack watch --config webpack.dev.cjs\" \"graphql-codegen --config graphql-codegen.yml --watch\"",
    "codegen": "graphql-codegen --config graphql-codegen.yml --verbose"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cuadue/MPD.FM.git"
  },
  "keywords": [
    "MPD",
    "client",
    "server"
  ],
  "author": "Florian Heinemann",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cuadue/MPD.FM/issues"
  },
  "homepage": "https://github.com/cuadue/MPD.FM#readme",
  "dependencies": {
    "@apollo/client": "^3.8.4",
    "@apollo/server": "^4.6.0",
    "@babel/core": "^7.22.10",
    "@babel/preset-env": "^7.22.10",
    "@babel/preset-react": "^7.22.5",
    "@cuadue/mpd": "^2.1.0",
    "@reduxjs/toolkit": "^1.9.5",
    "body-parser": "^1.20.2",
    "css-loader": "^6.8.1",
    "graphql": "^16.8.1",
    "graphql-ws": "^5.14.0",
    "html-webpack-plugin": "^5.5.3",
    "morgan": "~1.9.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.1.2",
    "react-router-dom": "^6.15.0",
    "serve-favicon": "~2.4.5",
    "source-map-loader": "^4.0.1",
    "source-map-support": "^0.5.21",
    "style-loader": "^3.3.3",
    "tiny-typed-emitter": "^2.1.0",
    "ws": "^8.14.1"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "5.0.0",
    "@graphql-codegen/client-preset": "^4.1.0",
    "@graphql-codegen/typescript": "4.0.1",
    "@graphql-codegen/typescript-document-nodes": "^4.0.1",
    "@graphql-codegen/typescript-operations": "^4.0.1",
    "@parcel/watcher": "^2.3.0",
    "@tsconfig/node18": "^2.0.0",
    "@types/cors": "^2.8.13",
    "@types/debug": "^4.1.8",
    "@types/node": "^20.6.3",
    "@types/react": "^18.2.22",
    "@types/react-dom": "^18.2.7",
    "babel-loader": "^9.1.3",
    "babel-plugin-source-map-support": "^2.2.0",
    "concurrently": "^8.0.1",
    "favicons-webpack-plugin": "^6.0.1",
    "nodemon": "^2.0.22",
    "ts-loader": "^9.4.4",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4"
  },
  "nodemonConfig": {
    "ext": "graphql,ts"
  }
}
