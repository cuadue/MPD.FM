{
  "name": "mpd.fm-backend",
  "version": "0.2.0",
  "description": "A MPD web server and client to listen to your favorite online radio stations",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "concurrently \"npx tsx src/server.ts\" \"graphql-codegen --config graphql-codegen.yml --watch\"",
    "codegen": "graphql-codegen --config graphql-codegen.yml",
    "watch": "graphql-codegen --config graphql-codegen.yml --watch",
    "serve": "tsx src/server.tsx"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cuadue/MPD.FM.git"
  },
  "keywords": [
    "MPD",
    "client",
    "server"
  ],
  "author": "Florian Heinemann",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cuadue/MPD.FM/issues"
  },
  "homepage": "https://github.com/cuadue/MPD.FM#readme",
  "dependencies": {
    "@apollo/server": "^4.6.0",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "express": "~4.15.5",
    "fast-deep-equal": "^3.1.3",
    "graphql": "^16.6.0",
    "graphql-ws": "^5.14.0",
    "morgan": "~1.9.0",
    "nanoid": "^5.0.1",
    "serve-favicon": "~2.4.5",
    "tiny-typed-emitter": "^2.1.0",
    "ws": "^8.14.1"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "5.0.0",
    "@graphql-codegen/typescript": "4.0.1",
    "@graphql-codegen/typescript-resolvers": "4.0.1",
    "@parcel/watcher": "^2.3.0",
    "@tsconfig/node18": "^2.0.0",
    "@types/cors": "^2.8.13",
    "@types/debug": "^4.1.8",
    "@types/graphql": "^14.5.0",
    "concurrently": "^8.0.1",
    "nodemon": "^2.0.22",
    "ts-node": "^10.9.1",
    "tsx": "^3.12.10",
    "typescript": "^5.0.4"
  },
  "nodemonConfig": {
    "ext": "graphql,ts"
  }
}
