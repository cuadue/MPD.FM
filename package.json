{
  "name": "mpd.fm",
  "version": "0.2.0",
  "description": "A MPD web server and client to listen to your favorite online radio stations",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "start": "concurrently \"nodemon src/server.ts\" \"graphql-codegen --config codegen.json --watch\"",
    "codegen": "graphql-codegen --config codegen.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cuadue/MPD.FM.git"
  },
  "keywords": [
    "MPD",
    "client",
    "server"
  ],
  "author": "Florian Heinemann",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cuadue/MPD.FM/issues"
  },
  "homepage": "https://github.com/cuadue/MPD.FM#readme",
  "dependencies": {
    "@apollo/server": "^4.6.0",
    "@cuadue/mpd": "^1.4.2",
    "cors": "^2.8.5",
    "express": "~4.15.5",
    "graphql": "^16.6.0",
    "morgan": "~1.9.0",
    "serve-favicon": "~2.4.5",
    "tiny-typed-emitter": "^2.1.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "3.3.1",
    "@graphql-codegen/typescript": "3.0.4",
    "@graphql-codegen/typescript-resolvers": "3.2.1",
    "@tsconfig/node18": "^2.0.0",
    "@types/cors": "^2.8.13",
    "concurrently": "^8.0.1",
    "nodemon": "^2.0.22",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  },
  "nodemonConfig": {
    "ext": "graphql,ts"
  }
}
